package com.ump.automate.test.browser.impl;

import org.openqa.selenium.WebDriver;

import com.ump.automate.test.browser.SeleniumDriverInt;

/**
 * this class implements SeleniumDriverInt and performs method for getter and setter of webDriver
 * @author edward.cervantes
 */
public class SeleniumDriverImpl implements SeleniumDriverInt {

    private WebDriver seleniumWebDriver;

    @Override
    public WebDriver getDriver() {
        return this.seleniumWebDriver;
    }

    @Override
    public void setDriver(WebDriver driver) {
        this.seleniumWebDriver = driver;
    }

}
