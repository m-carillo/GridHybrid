package com.ump.automate.test.exception;

public class ExternalPropertiesException extends Exception {

    public static final String FIELD_ERROR = "Please check properties field: ";

    public static final String PROPERTIES_FILE_ERROR = "Properties file can't be found. Please check its path. ";

    public ExternalPropertiesException(String message) {
        super(message);
    }

    public ExternalPropertiesException(Throwable cause) {
        super(cause);
    }

    public ExternalPropertiesException(String message, Throwable cause) {
        super(message, cause);
    }
}
